{
  "ApplicationInsights":{
    "ConnectionString":""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging": "Information",
      "CarvedRock": "Debug"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    },
    "ApplicationInsights":{
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning",
        "CarvedRock":"Debug"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Critical"
      }
    }
  },
  "AllowedHosts": "*"
}
