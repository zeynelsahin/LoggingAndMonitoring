{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "CarvedRock": "Debug",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    },
    "Debug": {
      "LogLevel": {
        "Default":"Critical"
      }
    }
    //    "Debug": {
    //      "LogLevel": {
    //        "Default":"Debug",
    //        "Microsoft.AspNetCore":"Information"
    //      }
    //    }
  },
  "AllowedHosts": "*"
}
